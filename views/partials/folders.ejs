<h2>Folders</h2>
<div class="row">
    <div class="col-xs-6 col-sm-6 col-md-3 img-grid" ng-repeat="folder in folders | reverse">
    	<div ng-if="folders">
			<a href="#/folders/{{folder}}">
				<button type="button" class="btn btn-default btn-lg folder-button">
					<i class="fa fa-folder"></i>
					<span class="folder-name">{{folder}}</span>
				</button>
			</a>
    	</div>
	</div>
</div>